<body class="text-center ">
  <%-include('./inc/navbar') %>
  <main id="loginForm"  class="form-signin w-100 m-auto ">
    <form action="" method="post">
      <img class="mb-4" src="/docs/5.2/assets/brand/bootstrap-logo.svg" alt="" width="72" height="57">
      <h1 class="h3 mb-3 fw-normal"> Lütfen Oturum Açın!</h1>
     
      <div class="form-floating">
        <input type="email" name="email" class="form-control" id="floatingInput" placeholder="example@gmail.com">
       
      </div>

      <br>
      <div class="form-floating">
        <input type="password" name="password" class="form-control" id="floatingPassword" placeholder="Şifreniz">
       
      </div>

      

      <div class="checkbox mb-3">
        <label>
          <input type="checkbox" value="remember-me"> Beni hatırla
        </label>
      </div>
      <button class="w-100 btn btn-lg btn-primary"  type="submit" value="gonder"><i class="bi bi-door-open-fill"></i> <strong>Giriş Yap</strong> </button>

    </form>
  </main>


</body>

